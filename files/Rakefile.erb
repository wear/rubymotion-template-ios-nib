# -*- coding: utf-8 -*-
$:.unshift("/Library/RubyMotion/lib")
require 'motion/project/template/ios'

# Require and prepare Bundler
require "bundler"
Bundler.require

Motion::Project::App.setup do |app|
  # Use `rake config' to see complete project settings.
  app.name = '<%= name %>'
  app.info_plist['UIMainStoryboardFile'] = 'Storyboard_iPhone'
  app.info_plist['UIMainStoryboardFile~ipad'] = 'Storyboard_iPad'
end

MotionBundler.setup